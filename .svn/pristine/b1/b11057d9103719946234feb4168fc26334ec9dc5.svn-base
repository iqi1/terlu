#importBox{
position: absolute;
width: 502px;
height: 301px;
display:none;
background: #F5F5F5;
top:30%;
left:40%;
z-index:1985;
border: 1px solid #999;
box-shadow:-3px 0 3px lightgray, /*×ó±ßÒõÓ°*/
 3px 0 3px lightgray, /*ÓÒ±ßÒõÓ°*/
 0 -3px 3px #ccc, /*¶¥²¿ÒõÓ°*/
 0 3px 3px #ccc;
}

.boxHead{
    width: 100%;
    height: 40px;
    border-bottom: 1px solid #ccc;
}
.boxBody{
    width: 100%;
    height: 223px;
    border-bottom:  1px solid #ccc;
}
.bottom{
    width: 100%;
    height: 40px;
}
.txt{
    font-size: 20px;
    padding: 10px;
    line-height: 40px;
}
.closed{
    position: absolute;
    right: 10px;
    margin-top: 10px;
    width: 20px;
    height: 20px;
    border: 50%;
    display: inline-block;
    border: 1px solid #ccc;
    font-size: 15px;
    text-align: center;
    color: #555;
    font-weight: bold;
    line-height: 20px;
    background-color: #ddd;
}
.closed:hover{
    background: gray;
}
.onload{
    color: #2fa4e7;
}
.wrapper-top{
    margin-top: 20px;
    margin-left: 20px;
}
.wrapper-bottom{
    margin-left: 100px;
    margin-top: 30px;
}

.last>p{
    margin-top: 3px;
}
.sureBtn{
    position: absolute;
    right:10px;
}
